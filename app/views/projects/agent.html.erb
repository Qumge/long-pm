<%= render 'projects/header' %>
<div class="row">
  <div class="box">
    <div class="box-body">
      <% if @project.agent.present? %>
        <%= render partial: '/agents/detail', locals: {agent: @project.agent}%>
      <% else %>
        未选择代理，选择或<%= link_to '【申请】', new_agent_path  %>一个新的代理商
      <% end %>
    </div>
    <div class="box-footer">
      <div class="pull-right no-margin">
        <%= link_to '返回', project_path(@project), class: 'btn btn-primary' %>
      </div>
    </div>
  </div>

</div>
