<div class="box-body">
  <table class="table table-bordered">
    <tr>
      <th>时间</th>
      <th>业务人员</th>
      <th>下单金额</th>
      <th>发货金额</th>
      <th>回款金额</th>
    </tr>
    <% if @users_array.present? %>
      <% @users_array.each do |data| %>
        <tr>
          <td><%= data[0] %></td>
          <td><%= data[1].name %></td>
          <td><%= (@applied_hash[data[0]].present? && @applied_hash[data[0]][data[1]].present?) ? @applied_hash[data[0]][data[1]] : 0 %></td>
          <td><%= (@deliver_hash[data[0]].present? && @deliver_hash[data[0]][data[1]].present?) ? @deliver_hash[data[0]][data[1]] : 0 %></td>
          <td><%= data[2] %></td>
        </tr>
      <% end %>
    <% else %>
      <tr>
        <td colspan="5" align="center">没有可显示的内容</td>
      </tr>
    <% end %>

  </table>
</div>
<div class="box-footer clearfix">
  <div class="pull-right pagination-sm no-margin">
    <%= paginate @users_array, remote: true, params: params%>
  </div>

</div>